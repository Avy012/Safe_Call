{
  "expo": {
    "name": "Safe_Call",
    "slug": "Safe_Call",
    "scheme": "safecall",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_no_text.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.anonymous.Safe_Call",
      "googleServicesFile": "./android/app/google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_no_text.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "exp+safecall"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo_no_text.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@livekit/react-native-expo-plugin",
        "@config-plugins/react-native-webrtc"
      ],
      "expo-secure-store"
    ],
    "experiments": {},
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "46a0025a-5165-4e9c-aca2-3b1924719ca6"
      }
    },
    "owner": "ave12"
  }
}
